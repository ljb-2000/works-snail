.showone {
    -webkit-animation: show 1.5s ease-in;
    -moz-animation: show 1.5s ease-in;
    -ms-animation: show 1.5s ease-in;
}

.showtwo {
    margin-left: -20px;
    -webkit-animation: shownext 3s ease-out 2s;
    -moz-animation: shownext 3s ease-out 2s;
    -ms-animation: shownext 3s ease-out 2s;
}

@-webkit-keyframes show {
    0% {
        font-size: 14px;
        opacity: 0;
        filter: alpha(opacity=0);
    }
    50% {
        font-size: 15px;
        opacity: 1;
        filter: alpha(opacity=100);
    }
    80% {
        font-size: 15px;
        opacity: 1;
        filter: alpha(opacity=100);
    }
    100% {
        font-size: 16px;
        opacity: 1;
        filter: alpha(opacity=100);
    }
}

@-webkit-keyframes shownext {
    0% {
        font-size: 14px;
        opacity: 0;
        filter: alpha(opacity=0);
    }
    50% {
        font-size: 15px;
        -webkit-transform: translate(20px, 0);
        -ms-transform: translate(20px, 0);
        transform: translate(20px, 0);
        opacity: 1;
        filter: alpha(opacity=100);
    }
    100% {
        font-size: 15px;
        opacity: 1;
        filter: alpha(opacity: 100);
    }
}

@-moz-keyframes show {
    0% {
        font-size: 14px;
        opacity: 0;
        filter: alpha(opacity=0);
    }
    50% {
        font-size: 15px;
        opacity: 1;
        filter: alpha(opacity=100);
    }
    80% {
        font-size: 15px;
        opacity: 1;
        filter: alpha(opacity=100);
    }
    100% {
        font-size: 16px;
        opacity: 1;
        filter: alpha(opacity=100);
    }
}

@-moz-keyframes shownext {
    0% {
        font-size: 14px;
        opacity: 0;
        filter: alpha(opacity=0);
    }
    50% {
        font-size: 15px;
        -moz-transform: translate(20px, 0);
        -ms-transform: translate(20px, 0);
        opacity: 1;
        filter: alpha(opacity=100);
    }
    100% {
        font-size: 15px;
        opacity: 1;
        filter: alpha(opacity=100);
    }
}

