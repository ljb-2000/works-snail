[uwsgi]                                                                                                                                                              
socket = 127.0.0.1:9001
master = true
pythonpath = /opt/www/ujobs
module = ujobs.wsgi
processes = 32
daemonize = /var/log/uwsgi_web.log
buffer-size = 65535
pidfile = /tmp/uwsgi_web.pid
cpu-affinity = 1
memory-report = 1
threads = 32
listen = 128
post-buffering = 100M
stats = 127.0.0.1:1717
socket-timeout = 120
max-requests = 5000
disable-logging = true
;touch-reload=/opt/www/ujobs/ujobs.ini
;py-autoreload=3
;env=DJANGO_SETTINGS_MODULE=ujobs.settings