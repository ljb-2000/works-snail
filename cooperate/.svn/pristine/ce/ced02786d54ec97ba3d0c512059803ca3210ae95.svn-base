                                <div class="col-sm-10">
                                    <input type="text" class="col-sm-2 step-time">
                                    <input type="text" class="col-sm-2 step-time">
                                    <input type="text" class="col-sm-2 step-time">
                                    <input type="text" class="col-sm-2 step-time">
                                    <input type="text" class="col-sm-2 step-time last">
                                </div>
                            </div>
                            <div class="">
                                <div class="task-step">
                                    <div class="task-input">
                                        <!-- <div class="form-group">
                                            <label class="col-sm-2 control-label text-muted">上线时间：</label>
                                            <div class="input-group date date-step-one col-sm-2" data-date="" data-date-format="yyyy-mm-dd" data-link-field="dtp_input1" data-link-format="yyyy-mm-dd">
                                                <input id="j_start_time" class="form-control" size="16" type="text" value="" placeholder="上线时间" style="width:190px;" readonly>
                                                <span class="input-group-addon"><span class="glyphicon glyphicon-remove"></span></span>
                                                <span class="input-group-addon"><span class="glyphicon glyphicon-th"></span></span>
                                            </div>
                                            <input type="hidden" id="dtp_input1" value="">
                                        </div> -->
                                        <table class="table table-bordered table-center">
                                            <thead>
                                                <tr>
                                                    <th width="20%">时间段</th>
                                                    <th>步骤</th>
                                                    <th width="50%">内容</th>
                                                    <th>完成率</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr>
                                                    <td rowspan="4">2016.01.16 - 2016.01.22</td>
                                                    <td>1</td>
                                                    <td contenteditable="true">-</td>
                                                    <td contenteditable="true">-</td>
                                                </tr>
                                                <tr>
                                                    <td>2</td>
                                                    <td contenteditable="true">-</td>
                                                    <td contenteditable="true">-</td>
                                                </tr>
                                                <tr>
                                                    <td>3</td>
                                                    <td contenteditable="true">-</td>
                                                    <td contenteditable="true">-</td>
                                                </tr>
                                                <tr>
                                                    <td>4</td>
                                                    <td contenteditable="true">-</td>
                                                    <td contenteditable="true">-</td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                    <div class="text-center">
                                        <button class="btn btn-primary btn-input">
