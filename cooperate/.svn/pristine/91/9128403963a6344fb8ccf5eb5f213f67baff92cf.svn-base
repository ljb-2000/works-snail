html,
body {
    font-family: Arial, 'Helvetica Neue', 'Hiragino Sans GB', 'Microsoft YaHei', sans-serif;
    font-size: 14px;
    line-height: 1.6;
    color: #222;
}

body {
    min-width: 1200px;
    background: #e7e8eb;
}

a {
    color: #555;
}

a:hover {
    cursor: pointer;
    color: #555;
}

border-radius a:active,
a:focus {
    color: #555;
    outline: none !important;
}

ul {
    list-style: none;
}

input:focus,
select:focus,
textarea:focus {
    border-color: #ccc !important;
    outline: none !important;
    box-shadow: none !important;
}

button:focus {
    border-color: none !important;
    outline: none !important;
}

label {
    font-weight: normal;
}

textarea {
    resize: none !important;
}

table th {
    font-weight: normal;
    color: #999;
}

table th,
table td {
    font-size: 12px !important;
}

.form-control[disabled],
fieldset[disabled] .form-control {
    cursor: default !important;
}

.red {
    color: #bb1a0e;
}

.m0 {
    margin: 0 !important;
}

.m10 {
    margin: 10px !important;
}

.mt10 {
    margin-top: 10px;
}

.mt15 {
    margin-top: 15px;
}

.mt20 {
    margin-top: 20px;
}

.mt30 {
    margin-top: 30px;
}

.mb0 {
    margin-bottom: 0;
}

.mb20 {
    margin-bottom: 20px !important;
}

.mr10 {
    margin-right: 10px;
}

.mr35 {
    margin-right: 35px;
}

.mr82 {
    margin-right: 82px;
}

.ml35 {
    margin-left: 35px;
}

.pl0 {
    padding-left: 0;
}

.pb0 {
    padding-bottom: 0;
}

.pd0 {
    padding: 0;
}

.form-inline .dataTables_wrapper {
    clear: none !important;
}

.checknow {
    background: #d9edf7 !important;
}

.table-striped > tbody > tr:nth-of-type(odd) {
    background-color: #e7e7e7;
}

.pointer {
    cursor: pointer;
    color: #005380;
}

.panel,
.panel-heading,
.input-group-addon,
textarea,
input {
    border-radius: 0 !important;
}

.input-group-addon,
.form-control[readonly],
.form-control[disabled] {
    background: #fff;
}

.panel-group .panel-heading {
    text-align: center;
}


/* 提示窗样式 */

.a-layout {
    position: fixed;
    z-index: 9998;
    top: 0;
    left: 0;
    display: none;
    width: 100%;
    height: 100%;
    opacity: .3;
    background: rgb(0, 0, 0);
}

.a-msg {
    position: absolute;
    z-index: 9999;
    display: none;
    min-width: 140px;
    margin: 15px;
    padding: 10px;
    padding-left: 50px;
    cursor: pointer;
    border-radius: 5px;
}

.jSuccess {
    color: #264409;
    border: 1px solid #c6d880;
    background: #e6efc2 url('/static/images/success.png') no-repeat 15px center;
}

.jError {
    color: #8a1f11;
    border: 1px solid #fbc2c4;
    background: #fbe3e4 url('/static/images/error.png') no-repeat 15px center;
}

.jNotify {
    color: #35517c;
    border: 1px solid #a7c3f0;
    background: #d8e6fc url('/static/images/info.png') no-repeat 15px center;
}


/* 主体样式 */

[v-cloak] {
    display: none;
}

.header {
    position: relative;
    min-width: 1200px;
    border-top: 4px solid #44b549;
    border-bottom: 1px solid #d9dadc;
    background-color: #fff;
}

.inner-header {
    width: 1200px;
    height: 60px;
    margin: 0 auto;
}

.logo {
    font-size: 22px;
    float: left;
    margin: 0;
}

.logo a {
    line-height: 60px;
    display: block;
    overflow: hidden;
    width: 200px;
    height: 60px;
    margin-left: -13px;
    color: #696868;
}

.logo a:hover {
    text-decoration: none;
}

.logo-img {
    -webkit-transform: scale(.8);
    -moz-transform: scale(.8);
    transform: scale(.8);
    vertical-align: top;
}

.logo-text {
    margin-left: -8px;
}

.account {
    line-height: 60px;
    float: right;
}

.username {
    margin-right: 8px;
}

.exit {
    line-height: 17px;
    display: inline-block;
    height: 18px;
    padding-left: 11px;
    color: #222;
    border-left: 1px solid #ccc;
}

.exit:hover {
    cursor: pointer;
}

.main-box {
    width: 1200px;
    margin: 0 auto;
    padding: 36px 0;
}

.main-inner {
    min-height: 700px;
    border: 1px solid #d9dadc;
    background-color: #fff;
}

.col-side {
    position: relative;
    width: 15%;
    max-width: 208px;
    border-right: 1px solid #e7e7eb;
}

.col-main {
    width: 2000px;
    height: 700px;
}

.main-inner .col-side,
.main-inner .col-main {
    display: table-cell;
    vertical-align: top;
    word-wrap: break-word;
    word-break: break-all;
}

.menu-box {
    margin-bottom: 150px;
}

.menu {
    margin-bottom: 0;
    padding-top: 6px;
    border-top: 1px solid #e7e7eb;
}

.menu.menu-extra {
    margin-top: 13px;
    border-top-width: 0;
}

.menu-title {
    font-weight: normal;
    line-height: 34px;
    position: relative;
    overflow: hidden;
    width: auto;
    margin-bottom: 0;
    padding: 0 30px 0;
    white-space: nowrap;
    text-overflow: ellipsis;
    word-wrap: normal;
    color: #b2b2b2;
}

.menu-item {
    line-height: 34px;
}

.menu-item a {
    display: block;
    overflow: hidden;
    width: auto;
    padding: 0 0 0 58px;
    white-space: nowrap;
    text-overflow: ellipsis;
    word-wrap: normal;
    color: #222;
}

.menu-item a:hover {
    text-decoration: none;
}

.menu-item.selected a {
    color: #fff;
}

.menu-item:hover {
    background-color: #f4f5f9;
}

.menu-item.selected {
    background-color: #44b549;
}

.menu-icon {
    font-size: 16px;
    display: inline-block;
    width: 18px;
    height: 18px;
    margin-right: 6px;
    vertical-align: middle;
}

.copyright {
    font-size: 12px;
    line-height: 30px;
    position: absolute;
    bottom: 0;
    left: 30px;
    margin-bottom: 10px;
    color: #8d8d8d;
}

.main-title {
    font-size: 16px;
    font-weight: 400;
    font-style: normal;
    line-height: 75px;
    margin: 0;
    padding: 0 30px;
}

.nav-tabs > li.active > a,
.nav-tabs > li.active > a:focus,
.nav-tabs > li.active > a:hover {
    border-color: transparent;
}

.nav-tabs > li:hover {
    border-bottom: 3px solid #a0a3ab;
}

.nav-tabs > li.active {
    border-bottom: 3px solid #44b549;
}

.nav-tabs > li.first > a {
    padding-left: 30px;
}

.nav-tabs > li > a:hover {
    border-color: transparent;
    background-color: #fff;
}

.nav-tabs>li {
    border-bottom: 3px solid transparent;
    margin-top: 1px;
}

.tab-content > .tab-pane {
    padding: 20px 30px 40px 30px;
}

.btn.btn-input {
    min-width: 104px;
}

.i-inputform *:focus {
    outline: none !important;
}

.i-inputform input.ng-dirty.ng-invalid,
.i-inputform textarea.ng-dirty.ng-invalid {
    border: 1px solid #a94442;
}

.inpass {
    color: #b02c2c;
}

.pass {
    color: green;
}

.m-btn {
    margin: 0 82px 0 242px;
}

.datetime-form .input-group {
    float: left;
    padding-left: 15px;
}

.add {
    top: 5px;
    color: #44b549;
}

.remove {
    top: 5px;
    color: #882b2b;
}

.common-form {
    padding-top: 30px;
}

.list-box {
    padding: 30px;
    border-top: 1px solid #ccc;
    background: #fff;
}

.list-header {
    color: #9e9e9e;
}

.list-body > dl > dt {
    font-size: 14px;
    font-weight: bold;
    margin-bottom: 10px;
    color: #555;
}

.list-body > dl > dd {
    font-size: 14px;
    margin: 0 0 15px 0;
    color: #555;
}

.list-author,
.list-date {
    font-size: 14px;
    color: #999;
}

.list-edit {
    color: #4298ba;
}

.table-center th,
.table-center td {
    text-align: center;
    vertical-align: middle !important;
}

.mini-tips {
    line-height: 34px;
}

.mini-tips i {
    font-weight: 400;
    font-style: normal;
    margin-top: -3px;
    margin-right: 3px;
}

.icon-msg-mini {
    display: inline-block;
    width: 16px;
    height: 16px;
    vertical-align: middle;
}

.icon-msg-mini.info {
    background: url('/static/images/icon.png') 0 -1050px no-repeat;
}

.icon-save {
    background: url('/static/images/icon.png') 0 -4550px no-repeat;
}

.icon-save:hover {
    background: url('/static/images/icon.png') 0 -4576px no-repeat;
}

.icon-delete {
    background: url('/static/images/icon.png') 0 -4800px no-repeat;
}

.icon-delete:hover {
    background: url('/static/images/icon.png') 0 -4772px no-repeat;
}

.icon-download {
    background: url('/static/images/icon.png') 0 -4940px no-repeat;
}

.icon-download:hover {
    background: url('/static/images/icon.png') 0 -4968px no-repeat;
}

.icon-block {
    display: inline-block;
    width: 26px;
    height: 18px;
    vertical-align: middle;
}

.task-input,
.task-view {
    padding: 25px 25px 0 25px;
}

.task-step {
    margin-top: 41px;
    padding-top: 10px;
    border-top: 2px dotted #ccc;
}

.load-data {
    line-height: 40px;
    display: block;
    height: 40px;
    margin-top: 20px;
    text-align: center;
    text-decoration: none;
    color: #459ae9;
    background: #eee;
}

.load-data:hover {
    color: #459ae9;
}

.icon-down-open {
    display: inline-block;
    width: 12px;
    height: 9px;
    margin-right: 3px;
    vertical-align: middle;
    background: transparent url('/static/images/icon_up_down.png') no-repeat 0 0;
}

.doc-box {
    clear: both;
    padding: 0;
}

.doc-list dl dd {
    float: left;
    margin-left: 30px;
}

.doc-list {
    height: 40px;
    margin-bottom: 15px;
    padding: 11px;
    color: #555;
    background: #f5f5f5;
}

.doc-search {
    width: 350px;
}

.form-upload {
    padding: 50px 0 0 50px;
}

.step-btn .btn {
    width: 133px;
}

.panel-collapse .step-btn .btn {
    width: 129px;
}

.panel-heading {
    background: #fff !important;
}

.step-time {
    height: 34px;
    text-align: center;
    border-top: 1px solid #ccc;
    border-right: none;
    border-bottom: 1px solid #ccc;
    border-left: 1px solid #ccc;
}

.step-time.last {
    border-right: 1px solid #ccc;
}

.panel-collapse .panel-body:focus {
    outline: none;
}

.file-empty {
    font-size: 24px;
    padding-top: 100px;
    text-align: center;
    color: #ccc;
}

.content-left {
    text-align: left !important;
}

.input-td {
    width: 70px;
    text-align: center;
    border: none;
}

.input-content {
    width: 550px;
    text-align: center;
    border: none;
}

.input-content[type='text']:disabled,
.input-td[type='text']:disabled,
.input-time[type='text']:disabled {
    background: #fff;
}

.input-time {
    width: 150px;
    text-align: center;
    border: none;
}

.edit-month-btn {
    padding: 0 5px;
}

