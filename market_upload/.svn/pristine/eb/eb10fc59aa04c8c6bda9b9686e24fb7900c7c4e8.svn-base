            'onUploadSuccess' : function(file, data, response) {
            	/*$.ajax({
            		url: '',
            		type: 'GET',
            		data: file,
            	})
            	.done(function() {
            		$('#'+file.id).find('.fileName').prop('title',file.name);
            		$('#'+file.id).append('<div class="mt10"><span class="url">http://www.baidu.com</span><span class="pull-right"><a class="copy" href="javascript:void(0);" data-zclip-text="http://www.baidu.com">复制</a>&nbsp;&nbsp;<a href="javascript:void(0);">下载</a><span></div>');
            		console.log(file);
            	})
            	.fail(function() {

            	})    	*/
        	}
        });

    	$("body").on("copy", ".copy", function(e) {
	        e.clipboardData.clearData();
	        e.clipboardData.setData("text/plain", $(this).data("zclip-text"));
	        e.preventDefault();
	    });

	    $(document).off('click', '.copy').on('click', '.copy',function(){
	    	var txt = $(this).attr('data-zclip-text');
	    	jSuccess('复制成功!');
	    })
	});


</script>
</body>
</html>