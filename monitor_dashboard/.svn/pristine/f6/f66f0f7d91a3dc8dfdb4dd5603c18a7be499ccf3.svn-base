.main-box {
    padding: 20px 0 0 0;
}

.m-lists li {
    float: left;
    width: 18%;
    height: 120px;
    margin-right: 2.5%;
    background: #fff;
}

.m-lists li:nth-child(5) {
    margin-right: 0;
}

.m-lists-top {
    width: 100%;
    height: 90px;
}

.m-lists-bottom {
    width: 100%;
    height: 30px;
    padding: 5px 0;
    text-align: center;
    color: #fff;
}

.bg-01 {
    background: #f27a5c;
}

.bg-02 {
    background: #ebc367;
}

.bg-03 {
    background: #6ba0f8;
}

.bg-04 {
    background: #adc75d;
}

.bg-05 {
    background: #d589e2;
}

.table-center th,
.table-center td {
    text-align: center;
    font-size: 12px;
    vertical-align: middle;
    word-break: break-all;
}

.view-items {
    width: 32%;
    float: left;
}

.item-right {
    margin-right: 2%;
}

.creat-views .btn {
    width: 49%;
    margin-top: 5px;
    border: 1px solid #ccc;
}

.creat-views .btn.active {
    background-color: #F5F5F5;
}

.view-left-box {
    overflow: auto;
    min-height: 620px;
}

.view-left-box .panel-heading {
    border-bottom: none;
}

.view-right-box .panel-heading {
    border-bottom: none;
    background-color: #E4E0E0;
}

.view-right-box .panel {
    overflow: hidden;
}

.sticky {
    position: fixed;
    right: 0;
    top: 0;
    z-index: 9;
    min-width: 244px;
}

.v-center {
    vertical-align: middle !important;
    text-align: center;
}

.empty-font {
    color: #a7a7a7;
    line-height: 34px;
    text-align: center;
}

.view-left-box .table td {
    vertical-align: middle;
}

.animated {
    -webkit-animation-duration: 1s;
    animation-duration: 1s;
    -webkit-animation-fill-mode: both;
    animation-fill-mode: both
}

@-webkit-keyframes fadeInLeft {
    0% {
        opacity: 0;
        -webkit-transform: translate3d(-100%, 0, 0);
        transform: translate3d(-100%, 0, 0)
    }
    100% {
        opacity: 1;
        -webkit-transform: none;
        transform: none
    }
}

@keyframes fadeInLeft {
    0% {
        opacity: 0;
        -webkit-transform: translate3d(-100%, 0, 0);
        -ms-transform: translate3d(-100%, 0, 0);
        transform: translate3d(-100%, 0, 0)
    }
    100% {
        opacity: 1;
        -webkit-transform: none;
        -ms-transform: none;
        transform: none
    }
}

.fadeInLeft {
    -webkit-animation: fadeInLeft 1s;
    animation: fadeInLeft 1s;
}

@-webkit-keyframes fadeInUp {
    0% {
        opacity: 0;
        -webkit-transform: translate3d(0, 100%, 0);
        transform: translate3d(0, 100%, 0)
    }
    100% {
        opacity: 1;
        -webkit-transform: none;
        transform: none
    }
}

@keyframes fadeInUp {
    0% {
        opacity: 0;
        -webkit-transform: translate3d(0, 100%, 0);
        -ms-transform: translate3d(0, 100%, 0);
        transform: translate3d(0, 100%, 0)
    }
    100% {
        opacity: 1;
        -webkit-transform: none;
        -ms-transform: none;
        transform: none
    }
}

.fadeInUp {
    animation: fadeInUp 1s;
    -webkit-animation: fadeInUp 1s;
}
