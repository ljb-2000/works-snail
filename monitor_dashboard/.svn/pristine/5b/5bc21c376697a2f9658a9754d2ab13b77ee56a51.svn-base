}

.ani-unusual {
    -webkit-animation: lighting 1s ease infinite;
    -moz-animation: lighting 1s ease infinite;
    animation: lighting 1s ease infinite;
    background: #BB4646;
}

.ani-danger {
    -webkit-animation: warning 1s ease infinite;
    -moz-animation: warning 1s ease infinite;
    animation: warning 1s ease infinite;
    background: #d47878;
}

@keyframes lighting {
    0% {
        background: #fff;
    }
    100% {
        background: #BB4646;
    }
}

@-webkit-keyframes lighting {
    0% {
        background: #fff;
    }
    100% {
        background: #BB4646;
    }
}

@-moz-keyframes lighting {
    0% {
        background: #fff;
    }
    100% {
        background: #BB4646;
    }
}

@keyframes warning {
    0% {
        background: #fff;
    }
    100% {
        background: #EFC4C4;
    }
}

@-webkit-keyframes warning {
    0% {
        background: #fff;
    }
    100% {
        background: #EFC4C4;
    }
}

@-moz-keyframes warning {
    0% {
        background: #fff;
    }
    100% {
        background: #EFC4C4;
    }
}

