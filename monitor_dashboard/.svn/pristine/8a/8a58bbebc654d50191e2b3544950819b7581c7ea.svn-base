$(function() {

    // 选择屏幕动画效果
    (function() {
        $('.game-wrap li').hover(function() {
            var e = $(this).find('.showbar');

            e.next().stop().animate({"bottom": "0"}, 400);
        }, function() {
            var e = $(this).find('.showbar');

            e.next().stop().animate({"bottom": "-74px"}, 400);
        });
    })();
});
