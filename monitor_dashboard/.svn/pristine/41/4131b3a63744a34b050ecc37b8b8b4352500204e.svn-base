.main-box {
    padding: 20px 0;
}

.m-lists li {
    float: left;
    width: 18%;
    height: 120px;
    margin-right: 2.5%;
    background: #fff;
}

.m-lists li:nth-child(5) {
    margin-right: 0;
}

.m-lists-top {
    width: 100%;
    height: 90px;
}

.m-lists-bottom {
    width: 100%;
    height: 30px;
    padding: 5px 0;
    text-align: center;
    color: #fff;
}

.bg-01 {
    background: #f27a5c;
}

.bg-02 {
    background: #ebc367;
}

.bg-03 {
    background: #6ba0f8;
}

.bg-04 {
    background: #adc75d;
}

.bg-05 {
    background: #d589e2;
}

.table-center th,
.table-center td {
    text-align: center;
    font-size: 12px;
    vertical-align: middle;
    word-break: break-all;
}

.view-items {
    width: 32%;
    float: left;
}

.view-left {
    margin-left: 2%;
}