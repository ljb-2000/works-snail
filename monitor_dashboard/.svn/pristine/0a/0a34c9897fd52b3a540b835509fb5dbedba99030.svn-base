/** jNotify CSS **/
/*****************/
#jNotify {
    position:absolute;
    background:#d8e6fc  url('../src/tool/jNotify/info.png') no-repeat 15px center;
    color:#35517c;
    border:1px solid #a7c3f0;
    padding:10px;
    padding-left:50px;
    margin:15px;
    z-index:9999;
    -moz-border-radius : 5px;
    border-radius:5px;
    -webkit-border-radius:5px;
}
#jNotify a {color:#35517c !important;text-decoration:none;}

